{
  "name": "manettabag",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently \"npm run dev:css\" \"astro dev\"",
    "build": "npm run build:css && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "dev:css": "tailwindcss -i ./src/styles/global.css -o ./public/tailwind.css --watch",
    "build:css": "tailwindcss -i ./src/styles/global.css -o ./public/tailwind.css --minify"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.11",
    "@stripe/stripe-js": "^8.5.2",
    "astro": "^5.15.5",
    "stripe": "^20.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18"
  }
}
